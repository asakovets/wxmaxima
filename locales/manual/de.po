"    title=sconcat(\"α=\",α),\n"
"    surface_hide=true,\n"
"    contour=both,\n"
"    view=[60,α],\n"
"    explicit(\n"
"        sin(x)*sin(y),\n"
"        x,-π,π,\n"
"        y,-π,π\n"
"    )\n"
")$\n"
