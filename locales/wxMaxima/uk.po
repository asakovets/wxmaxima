#~ "    with_slider_draw(\n"
#~ "        a,[-16,-9,-4,-2,0,1,2,3,4,5,6,7],\n"
#~ "        title=concat(\"a=\",a),\n"
#~ "        grid=true,\n"
#~ "        explicit(\n"
